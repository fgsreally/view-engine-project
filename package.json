{
  "name": "@fgsreally/view-engine-project",
  "version": "1.0.0",
  "scripts": {
    "build": "pnpm --filter=@fgsreally/core run build",
    "publish": "pnpm --filter=@fgsreally/core run publish",
    "commit": "git-cz",
    "test": "vitest",
    "changeset": "changeset",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/supertest": "^2.0.12",
    "conventional-changelog": "^3.1.25",
    "deepcopy": "^2.1.0",
    "lodash": "^4.17.21",
    "minordb": "^0.0.5",
    "mitt": "^3.0.0",
    "nanoid": "^4.0.0",
    "typedoc": "^0.23.13",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "@cypress/vite-dev-server": "^3.0.0",
    "@cypress/vue": "^3.1.2",
    "@types/lodash": "^4.14.182",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.2",
    "commitizen": "^4.2.5",
    "cypress": "^10.2.0",
    "happy-dom": "^6.0.4",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vitest": "^0.21.1",
    "vue-tsc": "^0.34.7"
  }
}
